const
  TABLEN = 20;
  NAMELEN = 24;

type
  ntype = array[1:NAMELEN] of char;

var
  a : ntype;

procedure printName(b: ntype);
var
	i : integer;
begin
  write("Printing Name: \n");
  for i := 1 to 4 do
  	write(b[i]);
  end;
  write('\n');
end;

BEGIN
  a[1] := 'A';
  a[2] := 'l';
  a[3] := 'a';
  a[4] := 'n';

  write(a[1]);
  write(a[2]);
  write(a[3]);
  write(a[4], '\n');

  printName(a);
END.